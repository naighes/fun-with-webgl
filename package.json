{
  "name": "fun-with-webgl",
  "version": "0.0.1",
  "description": "just a gym with the purpose of learning webgl.",
  "main": "index.js",
  "dependencies": {
    "gl-matrix": "^2.3.2",
    "png.js": "^0.2.1"
  },
  "devDependencies": {
    "browserify": "^14.3.0",
    "concurrently": "^3.4.0",
    "http-server": "^0.10.0"
  },
  "scripts": {
    "build": "node_modules/browserify/bin/cmd.js index.js -o bundle.js",
    "start": "npm run build && npm run open",
    "open": "concurrently \"http-server -a localhost -p 8001\" \"open http://localhost:8001/index.html\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/naighes/fun-with-webgl.git"
  },
  "author": "Nicola Baldi (@naighes)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/naighes/fun-with-webgl/issues"
  },
  "homepage": "https://github.com/naighes/fun-with-webgl#readme"
}
